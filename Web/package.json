{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "proxy": "https://localhost:44398/",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.7",
    "connected-react-router": "^6.4.0",
    "history": "^4.9.0",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "seamless-immutable": "^7.1.4",
    "typeface-roboto": "0.0.75"
  },
  "scripts": {
    "lint:js": "eslint --max-warnings=0 --ext=js --ext=jsx --fix ./src",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "test:watch": "react-scripts test --env=jsdom --watch",
    "test:ci": "set CI=true&&react-scripts test --reporters=default --reporters=jest-junit --coverageReporters=cobertura --env=jsdom --coverage",
    "analyze": "source-map-explorer build/static/js/main.*",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --max-warnings=0 --ext=js --ext=jsx --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/stories/*.{js,jsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@types/node": "^13.13.45",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^2.3.0",
    "jest-junit": "^6.4.0",
    "jest-trx-results-processor": "^0.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.1",
    "react-scripts": "^4.0.3"
  }
}
